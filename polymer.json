{
	"lint": {
		"rules": [
			"polymer-2"
		]
	},
	"fragments": [
		"./src/todo-app.html"
	],
	"builds": [
		{
			"name": "modern",
			"preset": "es6-unbundled",
			"browserCapabilities": ["es2015", "push", "serviceworker"],
			"basePath": true,
			"swPrecacheConfig": "sw-precache-config.js"
		},
		{
			"name": "fallback",
			"preset": "es5-bundled",
			"html": {
				"minify": false
			},
			"basePath": true,
			"swPrecacheConfig": "sw-precache-config.js"
		}
	]
}
